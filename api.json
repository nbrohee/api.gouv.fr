---
layout: null
---
[
  {% for api in site.api %}
    {
      "title" : "{{ api.name }}",
      "url": "{{ api.url | prepend: site.baseurl }}",
      "description" : "{{ api.tagline }}",
      "image" : "{{ api.logo | prepend: site.baseurl }}",
      "domain" : "{{ api.domain }}",
      "owner" : "{{ api.owner }}",
      "contract" : "{{ api.contract }}",
      "category" : "{{ api.category }}",
      "keywords": [
        {% if api.keywords %}
        "{{ api.keywords | join: '","' }}"
        {% endif %}
      ],
      "clients": [
        {% if api.clients %}
        "{{ api.clients | join: '","' }}"
        {% endif %}
      ],
      "partners": [
        {% if api.partners %}
        "{{ api.partners | join: '","' }}"
        {% endif %}
      ]
    }
    {% if forloop.last == false %}
    ,
    {% endif %}
  {% endfor %}
]
